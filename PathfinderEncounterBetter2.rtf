DELIMITER $$\par
USE `PathfinderEncounter`$$\par
CREATE DEFINER = CURRENT_USER TRIGGER `PathfinderEncounter`.`Book_BEFORE_INSERT` BEFORE INSERT ON `Book` FOR EACH ROW\par
    Begin\par
    \par
        \tab DECLARE msg VARCHAR(255);\par
    If new.thirdparty < 0 or new.thirdparty > 1 then\par
\tab\tab set msg = "Error: Monster can either be from Third party or not.";\par
        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = msg;\par
        END IF;\par
        END;$$\par
\par
USE `PathfinderEncounter`$$\par
CREATE DEFINER = CURRENT_USER TRIGGER `PathfinderEncounter`.`Book_BEFORE_UPDATE` BEFORE UPDATE ON `Book` FOR EACH ROW\par
        Begin\par
    \par
        \tab DECLARE msg VARCHAR(255);\par
    If new.thirdparty < 0 or new.thirdparty > 1 then\par
\tab\tab set msg = "Error: Monster can either be from Third party or not.";\par
        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = msg;\par
        END IF;\par
        END;$$\par
\par
USE `PathfinderEncounter`$$\par
CREATE DEFINER = DatabaseManager TRIGGER `PathfinderEncounter`.`Monster_BEFORE_INSERT` BEFORE INSERT ON `Monster` \par
FOR EACH ROW\par
Begin\par
\tab DECLARE msg VARCHAR(255);\par
    DECLARE found_it INT;\par
    IF NEW.size < -4 OR NEW.size > 4 THEN\par
\tab\tab set msg = "Error: Size categories must be an integer between -4 and 4";\par
        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = msg;\par
        END IF;\par
        \par
\tab IF NEW.alignment <> 'LG' AND NEW.alignment <> 'NG' AND NEW.alignment <> 'CG' AND NEW.alignment <> 'LN' AND NEW.alignment <> 'N' AND NEW.alignment <> 'CN' AND NEW.alignment <> 'LE' AND NEW.alignment <> 'NE' AND NEW.alignment <> 'CE' Then\par
        set msg = "Error: Invalid input for alignment";\par
        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = msg;\par
        END IF;\par
        \par
    IF NEW.CR < 1 OR NEW.CR > 10 THEN\par
\tab\tab set msg = "Error: CR must be an integer between 1 and 10";\par
        SIGNAL SQLSTATE '45000' SET MESSAGE_TEXT = msg;\par
        END IF;   \par
        \par
    IF NEW.Armor < 0 OR NEW.Shield < 0 OR New.Deflection < 0 OR NEW.NaturalArmor < 0 OR NEW.Dodge < 0 OR New.MiscAC < 0 Then   \par
        set msg = "Error: All Armor Class values must be greater than 0";\par
